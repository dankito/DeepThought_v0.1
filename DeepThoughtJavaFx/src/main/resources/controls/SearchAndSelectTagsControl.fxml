<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import org.controlsfx.control.textfield.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root fx:id="pnContent" maxHeight="230.0" minHeight="230.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="pnFilterTags" alignment="CENTER_LEFT" prefHeight="40.0">
         <children>
            <CustomTextField fx:id="txtfldFilterTags" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnCreateTag" mnemonicParsing="false" onAction="#handleButtonCreateTagAction" text="%new...">
               <HBox.margin>
                  <Insets left="6.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="6.0" top="6.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="lstvwAllTags" editable="true" minHeight="150.0" />
   </children>
</fx:root>
