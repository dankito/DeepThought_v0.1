<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import org.controlsfx.control.textfield.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root maxWidth="1.7976931348623157E308" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
      <children>
          <HBox fx:id="paneSeriesTitleOrReference" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="40.0" prefHeight="40.0">
              <children>
                  <Label minWidth="80.0" prefHeight="40.0" prefWidth="80.0" text="%reference">
                      <HBox.margin>
                          <Insets right="6.0" />
                      </HBox.margin>
                  </Label>
                  <ComboBox fx:id="cmbxSeriesTitleOrReference" editable="true" maxWidth="1.7976931348623157E308" minWidth="40.0" prefHeight="34.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </ComboBox>
            <HBox fx:id="paneReferenceIndicationSettings" prefHeight="40.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="72.0" text="%indication">
                     <HBox.margin>
                        <Insets left="12.0" right="6.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtfldReferenceIndication" minWidth="60.0" prefHeight="34.0" prefWidth="90.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
              </children>
          </HBox>
      </children>
  </fx:root>
