<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import org.controlsfx.control.textfield.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root minWidth="375.0" type="TitledPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox maxWidth="1.7976931348623157E308" prefHeight="286.0">
            <children>
            <HBox fx:id="paneSearchForReference" alignment="CENTER_LEFT">
               <children>
                  <Label prefHeight="40.0" text="%search">
                     <HBox.margin>
                        <Insets right="6.0" />
                     </HBox.margin>
                  </Label>
                  <CustomTextField fx:id="txtfldSearchForReference" prefHeight="35.0" HBox.hgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets top="6.0" />
               </VBox.margin>
            </HBox>
            <ListView fx:id="lstvwReferences" minHeight="200.0" prefHeight="200.0">
               <VBox.margin>
                  <Insets top="6.0" />
               </VBox.margin>
            </ListView>
            </children>
        </VBox>
   </content>
   <graphic>
          <HBox fx:id="paneSeriesTitleOrReference" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="32.0" prefHeight="32.0">
              <children>
                  <Label fx:id="lblReference" minWidth="45.0" text="%reference">
                      <HBox.margin>
                          <Insets right="12.0" />
                      </HBox.margin>
                  </Label>
            <HBox fx:id="paneSelectedReferenceBase" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minWidth="40.0" HBox.hgrow="ALWAYS" />
            <HBox fx:id="paneReferenceIndicationSettings" alignment="CENTER_LEFT">
               <children>
                  <Label minWidth="45.0" text="%indication">
                     <HBox.margin>
                        <Insets left="12.0" right="6.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtfldReferenceIndication" minHeight="28.0" minWidth="60.0" prefWidth="90.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
              </children>
          </HBox>
   </graphic>
</fx:root>
