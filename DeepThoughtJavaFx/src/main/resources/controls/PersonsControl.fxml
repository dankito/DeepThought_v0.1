<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import org.controlsfx.control.textfield.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root animated="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.control.TitledPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <HBox fx:id="pnPersonsGraphic" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="22.0">
         <children>
            <Label text="%persons">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Label>
            <FlowPane fx:id="pnSelectedPersonsPreview" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" vgap="2.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </graphic>
   <content>
      <VBox fx:id="pnContent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0">
         <children>
            <HBox fx:id="hboxSearchForPerson" alignment="CENTER_LEFT" prefHeight="32.0">
               <children>
                  <Label text="%search">
                     <HBox.margin>
                        <Insets right="6.0" />
                     </HBox.margin>
                  </Label>
                  <CustomTextField fx:id="txtfldSearchForPerson" promptText="search.for.person" HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnNewPerson" minWidth="80.0" mnemonicParsing="false" onAction="#handleButtonNewPersonAction" text="%new...">
                     <HBox.margin>
                        <Insets left="6.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </HBox>
            <ListView fx:id="lstvwAllPersons" maxHeight="1.7976931348623157E308" minHeight="150.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </content>
</fx:root>
